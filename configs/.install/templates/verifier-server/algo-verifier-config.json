{
   "apiKeys": [
      {
         "name": "test1",
         "apiKey": "$(ALGOApiKey1)"
      },
      {
         "name": "test2",
         "apiKey": "$(ALGOApiKey1)"
      }
   ],
   "port": $(ALGOVerifierRouterServerPort),
   "checkAliveIntervalMs": 5000,
   "sourceId": "ALGO",
   "attestationTypes": [
      "Payment",
      "BalanceDecreasingTransaction",
      "ConfirmedBlockHeightExists",
      "ReferencedPaymentNonexistence"
   ],
   "indexerDatabase": {
      "hostname": "$(IndexerDatabaseHost)",
      "port": $(IndexerDatabasePort),
      "database": "$(IndexerDatabase)",
      "username": "$(IndexerReaderUsername)",
      "password": "$(IndexerReaderPassword)"
   },
   "chainConfiguration": {
      "name": "ALGO",
      "mccCreate": {
         "algod": {
            "url": "$(ALGOURLAlgod)",
            "token": "$(ALGOTokenAlgod)"
         }
      },
      "rateLimitOptions": {
         "maxRPS": 300,
         "timeoutMs": 5000,
         "retries": 3
      },
      "numberOfConfirmations": 3,
      "syncReadAhead": 30,
      "blockCollecting": "raw",
      "minimalStorageHistoryDays": 2,
      "minimalStorageHistoryBlocks": 100,
      "maxRequestsPerSecond": 80,
      "maxProcessingTransactions": 3000,
      "maxFailedRetry": 1,
      "delayBeforeRetry": 10,
      "maxValidIndexerDelaySec": 10,
      "reverificationTimeOffset": 10
   }
}