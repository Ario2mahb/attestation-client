{
   "type": "sqlite",
   "database": "./data/flaredb.sq3",
   "synchronize": false,
   "logging": false,
   "entities": [
        "src/entity/*{.ts,.js}",
        "src/entity/aggregates/*{.ts,.js}"
],
   "migrations": [
      "src/migration/**/*{.ts,.js}"
   ],
   "subscribers": [
      "src/subscriber/**/*{.ts,.js}"
   ],
   "cli": {
      "entitiesDir": "src/entity; src/entity/aggregates",
      "migrationsDir": "src/migration",
      "subscribersDir": "src/subscriber"
   }
}